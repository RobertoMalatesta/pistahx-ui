<div class="container-fluid">
    <!-- Page Heading -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">
                {{model}}
            </h1>
            <ol class="breadcrumb">
                <li>
                    <i class="fa fa-dashboard"></i>  <a href="Javascript:void(0)" (click)="gotoDashboard()">Dashboard</a>
                </li>
                <li class="active">
                    <i class="fa fa-edit"></i> {{model}}s
                </li>
            </ol>
        </div>
    </div>
    <!-- /.row -->

    <div class="row">
        <div class="col-lg-6">
        <table class="table table-bordered table-hover table-striped">
            <thead>
                <tr>
                    <th>#ID {{model}}</th>
                    <th>title</th>
                </tr>
            </thead>
            <tbody>
                <tr *ngFor="#{{model_lowcase}} of {{model_lowcase}}s">
                    <td><a href="#/dashboard/{{model_lowcase}}/{{=<% %>=}}{{ <%model_lowcase%>.id<%model%> }}<%={{ }}=%>">{{=<% %>=}}{{ <%model_lowcase%>.id<%model%> }}<%={{ }}=%></a></td>
                    <td><a href="#/dashboard/{{model_lowcase}}/{{=<% %>=}}{{ <%model_lowcase%>.id<%model%> }}<%={{ }}=%>">{{=<% %>=}}{{ <%model_lowcase%>.code<%model%> }}<%={{ }}=%></a></td>
                </tr>
            </tbody>
        </table>
        </div>
    </div>
    <!-- /.row -->

</div>