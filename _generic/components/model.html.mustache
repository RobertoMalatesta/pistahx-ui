<div class="container-fluid">
    <!-- Page Heading -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">
                {{model}}
            </h1>
            <ol class="breadcrumb">
                <li>
                    <i class="fa fa-dashboard"></i>  <a href="Javascript:void(0)" (click)="gotoDashboard()">Dashboard</a>
                </li>
                <li class="active">
                    <i class="fa fa-edit"></i> {{model}}
                </li>
            </ol>
        </div>
    </div>
    <!-- /.row -->

    <div class="row">
        <div class="col-lg-6">
         
            <form [ngFormModel]="{{model_lowcase}}Form"  (submit)="add{{model}}()"> 
                <div ng-show="creationDisplay" class="form-group" >
                  <label for="id{{model}}">Id</label>
                  <input type="text" DISABLED class="form-control" ngControl="id{{model}}" value="{{=<% %>=}}{{id<%model%>}}<%={{ }}=%>" placeholder="Id {{model}}" >
                </div>
                <div class="form-group">
                  <label for="code{{model}}">Code</label>
                  <input type="text" class="form-control "ngControl="code{{model}}" placeholder="Enter {{model}} Code" value="{{=<% %>=}}{{code<%model%>}}<%={{ }}=%>">
                </div>
                <div class="form-group">
                  <label for="title">Title</label>
                  <input type="text" class="form-control" ngControl="title" placeholder="Enter {{model}} Title" value="{{title}}">
                </div>
                <div class="form-group">
                  <label for="{{model_lowcase}}Desc">Description</label>
                  <input type="text" class="form-control" ngControl="description" placeholder="Enter {{model}} Description" value="{{description}}">
                </div>
               
                <button type="submit" class="btn white m-b">Save</button>
            </form>

        </div>
    </div>
    <!-- /.row -->

</div>